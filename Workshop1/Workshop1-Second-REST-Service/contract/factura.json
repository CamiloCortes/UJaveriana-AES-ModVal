{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://json-schema.org/draft-07/schema#",
  "title": "Esquema para Factura",
  "type": "object",
  "properties": {
    "referenciaPago": {
      "type": "string"
    },
    "valorReferencia": {
      "type": "number"
    },
    "estadoReferencia": {
      "type": "string",
      "enum": [
        "PAGADA",
        "SIN_PAGAR"
      ]
    }
  },
  "required": ["referenciaPago", "valorReferencia", "estadoReferencia"],
  "additionalProperties": false
}